{
  "name": "listening-intelligence",
  "version": "0.1.0",
  "description": "A data visualization dashboard for personal music analytics built with Vite, React, TypeScript, and modern tooling",
  "author": "Trevor H <trevorh2007@example.com>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://trevorh2007.github.io/listening-intelligence",
  "repository": {
    "type": "git",
    "url": "https://github.com/trevorh2007/listening-intelligence.git"
  },
  "bugs": {
    "url": "https://github.com/trevorh2007/listening-intelligence/issues"
  },
  "keywords": [
    "data-visualization",
    "music-analytics",
    "dashboard",
    "vite",
    "react",
    "typescript",
    "web-development"
  ],
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --config config/eslint.config.cjs",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --config config/eslint.config.cjs",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --config config/eslint.config.js --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" --config config/prettier.config.js",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" --config config/prettier.config.js",
    "format:staged": "prettier --write --config config/prettier.config.js",
    "type-check": "tsc --noEmit",
    "test": "jest --config=config/jest/jest.config.cjs",
    "test:watch": "jest --config=config/jest/jest.config.cjs --watch",
    "test:coverage": "jest --config=config/jest/jest.config.cjs --coverage",
    "test:ci": "jest --config=config/jest/jest.config.ci.cjs --coverage --watchAll=false --passWithNoTests",
    "validate": "npm run type-check && npm run lint && npm run test:ci && npm run build"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "trevor-ui": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@swc/core": "^1.7.36",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.5.0",
    "eslint-plugin-testing-library": "^7.15.4",
    "gh-pages": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
